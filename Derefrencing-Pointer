#include <stdio.h>

int main()
{
    int a = 10, *p;
    float b = 5.5, *q;
    p = &a;
    q = &b;
    printf("Value of p = Address of a = %u\n", p);
    printf("Value of q = Address of b = %u\n", q);
    printf("Address of p = %u\n", &p);
    printf("Address of q = %u\n", &q);
    printf("Value of a = %d %d\n", a, *p, *(&a));
    printf("Value of b = %f %f\n", b, *q, *(&b));
}
